  "description": "",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/node": "22.13.8",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.0.7",
    "eslint": "8.57.1",
    "npm-run-all": "4.1.5",
    "prisma": "6.4.1",
    "supertest": "7.1.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.7"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.1.0",
